
main [
	layer {
		position: fixed;
		overflow: auto;
		left: 0;
		top: 0;
		width: 100%;
		height: 100%;
		overflow: auto;
	}
	center {
		margin: 0 auto 0 auto;
		padding: ${panel-padding-half} 0;
	}
	divider {
		table-layout: fixed;
	}
	menu {
		margin: ${panel-padding-half} 0;
	}
	user-name {
		display: block;
		float: left;
	}
]
